{
    "name": "Bookmarks Extension",
    "description": "Bookmarks Extension",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "start.html",
      "default_icon": "img/icon.png"
    },
    "permissions": [
        "storage",
        "bookmarks"
      ],
      "host_permissions": [
        "<all_urls>"
      ],
        "background": {
            "service_worker": "script/background.js"
        }
  }